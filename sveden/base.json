{

	// реестр квалификаций
	"qualifications": [
		{
			"qualification_id": "int - ключ",
			"title": "text[50] - наименование квалификации (техник, бакалавр, магистр, специалист, инженер, канд. наук, исследователь, и т. п.)"
		}
	],

	// реестр видов учебного процесса
	"learning_types": [
		{
			"learning_type_id": "int - ключ",
			"type": "text[50] - наименование вида учебного процесса",
			"type_short": "text[14] - сокращенное наименование вида учебного процесса"
		}
	],

	// реестр видов практик
	"practice_types": [
		{
			"practice_type_id": "int - ключ",
			"type": "text[250] - наименование вида практики"
		}
	],

	// реестр типов документов
	"doctypes": [
		{
			"doctype_id": "int - ключ",
			"title": "text[50] - название типа",
			"prefix": "text[50] - префикс имени файла"
		}
	],

	// реестр имен циклов
	"cycles": [
		{
			"cycle_id": "int - ключ",
			"cycle": "text[5] - код цикла",
			"title": "text[50] - название цикла"
		}
	],

	// уровни образования
	"levels": [
		{
			"level_id": "int - ключ",
			"disabled": "boolean - отключен (0=включен;1=отключен)",
			"title": "text[250] - наименование",
			"group": "text[250] - группа уровня образования"
		}
	],

	// направления подготовки
	"directions": [
		{
			"direction_id": "int - ключ",
			"level_ptr": "int - указатель на уровень образования [^levels]",
			"qualification_ptr": "int - указатель на квалификацию [^qualifications]",
			"disabledEdu": "boolean - не отображать направление/специальность",
			"code": "text[10] - код",
			"code_old": "text[10] - старый код",
			"title": "text[250] - наименование направления подготовки, профессии, специальности",
			"standards": [
				{
					"title": "text[10] - название стандарта {vo=ФГОС ВО;vo3pp=ФГОС ВО 3++;vpo=ФГОС ВПО}",
					"standard_url": "text[250] - ссылка на сайте министерства",
					"disabledStandart": "boolean - не отображать стандарт"
				}
			],
			"complexity": "int - трудоемкость, зачетные единицы",
			"date_end": "date - срок действия аккредитации (пусто, если нет)"
		}
	],

	// профили подготовки (направленность, специализация)
	"profiles": [
		{
			"profile_id": "int - ключ",
			"direction_ptr": "int - указатель на направление подготовки [^directions]",
			"title": "text[250] - наименование профиля подготовки (направленности, специализации)",
			"is_appled": "boolean - признак прикладного бакалавриата (иначе, академический бакалавриат)",
			"is_hidden": "boolean - признак скрытого профиля (не отображается в основной выгрузке)",
			"chair": "text[10] - выпускающая кафедра ('k01', 'k22', 'k43', 'k91', 'k101', 'k501' и т. д.)",
			"department": "text[10] - выпускающий институт/факультет ('i01', 'i05', 'f09', 'fibmp', 'indo', 'igf' и т. д.)",
			"abstracts": "text[MEMO] - краткое описание (текст в несколько абзацев, для сайта кафедры)",
			"file_reklama": "text[250] - хеш имени файла рекламы ОП для сайта кафедры",
			"file_reklama_ext": "text[10] - расширение файл рекламы ОП для сайта кафедры"
		}
	],

	// года приема + форма обучения
	"years": [
		{
			"year_id": "int - ключ",
			"profile_ptr": "int - указатель на профиль подготовки [^profiles]",
			"year": "int - год приема",
			"id_up": "int - идентификатор из УМО (для отладки)",
			"type": "int - формы обучения {0=нет;1=очная;2=очно-заочная;3=заочная}",
			"length": "text[20] - срок обучения",
			"abstracts": "text[MEMO] - краткое описание (текст в несколько абзацев, для сайта кафедры)",
			"file_oop": "text[250] - хеш имени файла описания образовательной программы",
			"file_oop_ext": "text[10] - расширение файла описания образовательной программы",
			"file_uchplan": "text[250] - хеш имени файла учебного плана (приоритетная копия для некоторых вместо генерации)",
			"file_uchplan_ext": "text[10] - расширение файла учебного плана (приоритетная копия для некоторых вместо генерации)",
			"file_calgr": "text[250] - хеш имени файла календарного учебного графика",
			"file_calgr_ext": "text[10] - расширение файла календарного учебного графика",
			"disabled_files": "boolean - признак образовательной программы с особыми условиями реализации (вместо контента выводится соответствующая запись)",
			"langs": "text[50] - языки преподавания (список через запятую, например: 'ru,en,ch,es'). Если поле пустое, то считается, что язык русский"
		}
	]

}
